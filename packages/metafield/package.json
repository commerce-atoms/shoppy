{
  "name": "@commerce-atoms/metafield",
  "version": "0.4.1",
  "description": "Pure metafield and metaobject extraction/parsing",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "shopify",
    "hydrogen",
    "metafield",
    "metaobject",
    "ecommerce",
    "utilities"
  ],
  "author": "Commerce Atoms",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/commerce-atoms/shoppy",
    "directory": "packages/metafield"
  },
  "homepage": "https://github.com/commerce-atoms/shoppy/tree/main/packages/metafield",
  "bugs": {
    "url": "https://github.com/commerce-atoms/shoppy/issues"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    "./package.json": "./package.json",
    "./metafields/getMetafield": {
      "types": "./dist/metafields/getMetafield.d.ts",
      "import": "./dist/metafields/getMetafield.js"
    },
    "./metafields/getMetafieldValue": {
      "types": "./dist/metafields/getMetafieldValue.d.ts",
      "import": "./dist/metafields/getMetafieldValue.js"
    },
    "./metaobjects/getMetaobjectReferenceFromMetafield": {
      "types": "./dist/metaobjects/getMetaobjectReferenceFromMetafield.d.ts",
      "import": "./dist/metaobjects/getMetaobjectReferenceFromMetafield.js"
    },
    "./metaobjects/getMetaobjectField": {
      "types": "./dist/metaobjects/getMetaobjectField.d.ts",
      "import": "./dist/metaobjects/getMetaobjectField.js"
    },
    "./metaobjects/getMetaobjectString": {
      "types": "./dist/metaobjects/getMetaobjectString.d.ts",
      "import": "./dist/metaobjects/getMetaobjectString.js"
    },
    "./metaobjects/getMetaobjectStringList": {
      "types": "./dist/metaobjects/getMetaobjectStringList.d.ts",
      "import": "./dist/metaobjects/getMetaobjectStringList.js"
    },
    "./metaobjects/getMetaobjectMediaImage": {
      "types": "./dist/metaobjects/getMetaobjectMediaImage.d.ts",
      "import": "./dist/metaobjects/getMetaobjectMediaImage.js"
    },
    "./metaobjects/getMetaobjectMediaImageList": {
      "types": "./dist/metaobjects/getMetaobjectMediaImageList.d.ts",
      "import": "./dist/metaobjects/getMetaobjectMediaImageList.js"
    },
    "./parse/parseMetafieldValue": {
      "types": "./dist/parse/parseMetafieldValue.d.ts",
      "import": "./dist/parse/parseMetafieldValue.js"
    },
    "./types/metafield": {
      "types": "./dist/types/metafield.d.ts",
      "import": "./dist/types/metafield.js"
    },
    "./types/metaobject": {
      "types": "./dist/types/metaobject.d.ts",
      "import": "./dist/types/metaobject.js"
    },
    "./types/parse": {
      "types": "./dist/types/parse.d.ts",
      "import": "./dist/types/parse.js"
    }
  },
  "dependencies": {},
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "pack:check": "npm pack --dry-run --silent",
    "prepublishOnly": "npm run build"
  }
}